{
  "name": "discordjs-template",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "description": "A template for Discord.js bots",
  "main": "src/app.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start:dev": "npm run lint && cross-env NODE_ENV=development node src/app.js",
    "start:prod": "cross-env NODE_ENV=production node src/app.js",
    "start": "npm run start:dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "setup-template": "node scripts/setup-template.js",
    "generate-config": "node scripts/generate-config.js",
    "pull-and-restart": "node scripts/pull-and-restart.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NullDev/DiscordJS-Template.git"
  },
  "bugs": {
    "url": "https://github.com/NullDev/DiscordJS-Template/issues"
  },
  "homepage": "https://github.com/NullDev/DiscordJS-Template#readme",
  "author": "NullDev",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/eslint-parser": "^7.24.5",
    "cross-env": "^7.0.3",
    "eslint": "^9.3.0"
  },
  "dependencies": {
    "better-sqlite3": "^10.0.0",
    "check-dependencies": "^2.0.0",
    "discord-hybrid-sharding": "^2.2.0",
    "discord.js": "^14.15.2",
    "i18n-light": "^0.5.4",
    "node-cron": "^3.0.3",
    "quick.db": "^9.1.7"
  }
}
